			<RADIO  concepto="Archivo configuración RF">RADIO.INI</RADIO>
	    </CONFIG>

	    <BBDD><COMVFP concepto="DSN Acceso a BBDD desde RF">COMVFP</COMVFP></BBDD>

		<VFP>
