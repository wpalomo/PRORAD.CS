<PRORAD>
	<COMMON>
		<PATHS>
			<PATHPROCAOT concepto="Carpeta de trabajo del SGA">D:/PROYECTOS/SGA.NET/PROCAOT.VFP</PATHPROCAOT>
			<PATHPRORAD  concepto="Carpeta de trabajo de RF">D:/PROYECTOS/SGA.NET/PRORAD.CS</PATHPRORAD>
	    </PATHS>

	    <FICHEROS>
			<MENU      concepto="Fichero BBDD de menús">MENUS</MENU>
			<PROGRAMAS concepto="Fichero BBDD de pantallas">PANTALLAS</PROGRAMAS>
			<MENSAJES  concepto="Fichero BBDD de mensajes">PRORAD</MENSAJES>
	    </FICHEROS>

	    <CONFIG>
			<CONNDB concepto="Archivo configuración BBDD SGA">CONNDB.INI</CONNDB>
