			<CONTROLTCL       concepto="Control Tono/Calibre/Lote">A</CONTROLTCL>
			<CONTROLCAD       concepto="Control Caducidad">A</CONTROLCAD>
		</DEFAULT>
			
		<CROSS-DOCKING    concepto="Configuración Cross-Docking">
			<MUELLESCD    concepto="Muelles de Cross-Docking">
				<MUELLECD concepto="Muelle Cross-Docking">0001CD01001010</MUELLECD>
			</MUELLESCD>

			<OPERARIOSCD    concepto="Operarios de Cross-Docking">
				<OPERARIOCD concepto="Operario Cross-Docking" activo="S">
					<CODIGO>0001</CODIGO>
					<NOMBRE>OPERARIO 1</NOMBRE>
				</OPERARIOCD>

				<OPERARIOCD concepto="Operario Cross-Docking" activo="N">
					<CODIGO>0006</CODIGO>
					<NOMBRE>OPERARIO 6</NOMBRE>
				</OPERARIOCD>
			</OPERARIOSCD>
		</CROSS-DOCKING>
	</COMMON>
